<h1>Events</h1>

<% if @events.any? %>
  <% @events.each do |event| %>
    <div>
      <h4><%= event.title %></h4>
      <p><%= event.description %></p>
      <p><%= event.start_date %></p>
      <p><%= event.end_date %></p>
      <p><%= event.venue %></p>
      <p><%= event.location %></p>
  </div>
  <% end %>
<% else %>
  <p>There are no events yet.</p>
<% end %>

<br>

<%= link_to 'Add Event', new_event_path %>
