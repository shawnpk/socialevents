<h1>Events</h1>

<%= link_to 'Add Event', new_event_path %>

<% @events.each do |event| %>
  <div>
    <h4><%= link_to event.title, event_path(event) %></h4>
    <p><%= event.description %></p>
    <p><%= event.start_date %></p>
    <p><%= event.end_date %></p>
    <p><%= event.venue %></p>
    <p><%= event.location %></p>
  </div>
<% end %>
